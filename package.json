{
  "name": "@co.mmons/angular-extensions",
  "version": "2.0.0",
  "author": "co.mmons",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "rm -Rf dist/**/* && npx ts-node --project build/tsconfig.json build/build.ts",
    "build:module": "npx ts-node --project build/tsconfig.json build/build.ts",
    "intl": "npm run intl:forms",
    "intl:forms": "js-intl-google-sheet-importer --output-path src/forms/i18n --output-type json --document 1lTpcwU0ubFX7bJFCt8zeHYrwbDaoxNcjzpjUVdmiY_w --worksheet 1 --filterTags angular-extensions-forms",
    "install": "node-utils-npm-install-dist"
  },
  "peerDependencies": {
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/platform-browser": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@co.mmons/js-intl": "git+https://github.com/co-mmons/js-intl.git",
    "@co.mmons/js-utils": "git+https://github.com/co-mmons/js-utils.git",
    "rxjs": "^6.5.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@co.mmons/node-utils": "git+https://github.com/co-mmons/node-utils.git",
    "@types/yargs": "^13.0.3",
    "bignumber.js": "^9.0.0",
    "ng-packagr": "^5.7.1",
    "typescript": "~3.4.0"
  },
  "sideEffects": false
}
