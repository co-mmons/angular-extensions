{
  "name": "@co.mmons/angular-extensions",
  "version": "2.0.0",
  "author": "co.mmons",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "rm -Rf dist/**/* && npx ts-node --project build/tsconfig.json build/build.ts",
    "build:module": "npx ts-node --project build/tsconfig.json build/build.ts",
    "intl": "npm run intl:forms",
    "intl:forms": "js-intl-google-sheet-importer --output-path src/forms/i18n --output-type json --document 1lTpcwU0ubFX7bJFCt8zeHYrwbDaoxNcjzpjUVdmiY_w --worksheet 1 --filterTags angular-extensions-forms",
    "install": "node-utils-npm-install-dist"
  },
  "peerDependencies": {
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/router": "^12.0.0",
    "@co.mmons/js-intl": "github:co-mmons/js-intl",
    "@co.mmons/js-utils": "github:co-mmons/js-utils",
    "@co.mmons/node-utils": "github:co-mmons/node-utils",
    "rxjs": "^7.0.0",
    "zone.js": "^0.11.0"
  },
  "devDependencies": {
    "@types/yargs": "^13.0.3",
    "bignumber.js": "^9.0.0",
    "ng-packagr": "^12.0.0",
    "typescript": "^4.2.0",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/router": "^12.0.0",
    "@co.mmons/js-intl": "github:co-mmons/js-intl",
    "@co.mmons/js-utils": "github:co-mmons/js-utils",
    "@co.mmons/node-utils": "github:co-mmons/node-utils",
    "rxjs": "^7.0.0",
    "zone.js": "^0.11.0"
  },
  "sideEffects": false
}
